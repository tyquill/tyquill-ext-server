{
  "family": "tyquill-ext-nestjs-service",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::305362906205:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "tyquill-nest-js-container",
      "image": "305362906205.dkr.ecr.ap-northeast-2.amazonaws.com/tyquill-ext-server:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/tyquill-ext-nestjs-service",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}